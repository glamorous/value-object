{
    "name": "glamorous/value-object",
    "description": "Interface to have ValueObjects in your application. An Enum implementing this interface is also available.",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "myclabs/php-enum": "1.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpro/grumphp": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.13@dev",
        "jakub-onderka/php-parallel-lint": "dev-master",
        "phpmd/phpmd": "dev-master",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^4.0@dev",
        "sensiolabs/security-checker": "^5.0@dev",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpstan/phpstan": "^0.10.0@dev",
        "povils/phpmnd": "^2.0@dev",
        "maglnet/composer-require-checker": "^1.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas De Smet",
            "email": "jonas@glamorous.be"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Glamorous\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Glamorous\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
